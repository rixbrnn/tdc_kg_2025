@prefix :     <http://example.org/chinook#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

{
    <http://example.org/chinook>
        a owl:Ontology ;
        rdfs:label "Chinook Knowledge Graph Ontology" ;
        rdfs:comment "Ontology for the Chinook sample database (customers, invoices, tracks, employees, etc.)." .

    ##########
    # Classes
    ##########

    :Customer      a owl:Class ; rdfs:label "Customer" .
    :Invoice       a owl:Class ; rdfs:label "Invoice" .
    :InvoiceLine   a owl:Class ; rdfs:label "Invoice line" .
    :Track         a owl:Class ; rdfs:label "Track" .
    :Album         a owl:Class ; rdfs:label "Album" .
    :Artist        a owl:Class ; rdfs:label "Artist" .
    :Genre         a owl:Class ; rdfs:label "Genre" .
    :MediaType     a owl:Class ; rdfs:label "Media type" .
    :Employee      a owl:Class ; rdfs:label "Employee" .
    :Playlist      a owl:Class ; rdfs:label "Playlist" .
    :PlaylistTrack a owl:Class ; rdfs:label "Playlist track" .

    ###################
    # Object properties
    ###################

    :hasInvoice a owl:ObjectProperty ;
        rdfs:label "has invoice" ;
        rdfs:domain :Customer ;
        rdfs:range  :Invoice .

    :hasLine a owl:ObjectProperty ;
        rdfs:label "has invoice line" ;
        rdfs:domain :Invoice ;
        rdfs:range  :InvoiceLine .

    :lineTrack a owl:ObjectProperty ;
        rdfs:label "line track" ;
        rdfs:domain :InvoiceLine ;
        rdfs:range  :Track .

    :hasAlbum a owl:ObjectProperty ;
        rdfs:label "has album" ;
        rdfs:domain :Track ;
        rdfs:range  :Album .

    :hasArtist a owl:ObjectProperty ;
        rdfs:label "has artist" ;
        rdfs:domain :Album ;
        rdfs:range  :Artist .

    :hasGenre a owl:ObjectProperty ;
        rdfs:label "has genre" ;
        rdfs:domain :Track ;
        rdfs:range  :Genre .

    :hasMediaType a owl:ObjectProperty ;
        rdfs:label "has media type" ;
        rdfs:domain :Track ;
        rdfs:range  :MediaType .

    :supportedBy a owl:ObjectProperty ;
        rdfs:label "supported by" ;
        rdfs:domain :Customer ;
        rdfs:range  :Employee .

    :reportsTo a owl:ObjectProperty ;
        rdfs:label "reports to" ;
        rdfs:domain :Employee ;
        rdfs:range  :Employee ;
        owl:inverseOf :manages .

    :manages a owl:ObjectProperty ;
        rdfs:label "manages" ;
        rdfs:domain :Employee ;
        rdfs:range  :Employee .

    :playlistTrack a owl:ObjectProperty ;
        rdfs:label "playlist track" ;
        rdfs:domain :PlaylistTrack ;
        rdfs:range  :Track .

    :inPlaylist a owl:ObjectProperty ;
        rdfs:label "in playlist" ;
        rdfs:domain :PlaylistTrack ;
        rdfs:range  :Playlist .

    ##################
    # Data properties
    ##################

    :customerId a owl:DatatypeProperty ;
        rdfs:label "customer id" ;
        rdfs:domain :Customer ;
        rdfs:range  xsd:integer .

    :employeeId a owl:DatatypeProperty ;
        rdfs:label "employee id" ;
        rdfs:domain :Employee ;
        rdfs:range  xsd:integer .

    :invoiceId a owl:DatatypeProperty ;
        rdfs:label "invoice id" ;
        rdfs:domain :Invoice ;
        rdfs:range  xsd:integer .

    :invoiceDate a owl:DatatypeProperty ;
        rdfs:label "invoice date" ;
        rdfs:domain :Invoice ;
        rdfs:range  xsd:dateTime .

    :total a owl:DatatypeProperty ;
        rdfs:label "invoice total" ;
        rdfs:domain :Invoice ;
        rdfs:range  xsd:decimal .

    :unitPrice a owl:DatatypeProperty ;
        rdfs:label "unit price" ;
        rdfs:domain :InvoiceLine ;
        rdfs:range  xsd:decimal .

    :quantity a owl:DatatypeProperty ;
        rdfs:label "quantity" ;
        rdfs:domain :InvoiceLine ;
        rdfs:range  xsd:integer .

    :title a owl:DatatypeProperty ;
        rdfs:label "job title" ;
        rdfs:domain :Employee ;
        rdfs:range  xsd:string .

    :fullName a owl:DatatypeProperty ;
        rdfs:label "full name" ;
        rdfs:range  xsd:string .

    :name a owl:DatatypeProperty ;
        rdfs:label "name" ;
        rdfs:range  xsd:string .

    ########################
    # Restricoes de exemplo
    ########################

    # Cada linha da fatura tem exatamente uma faixa
    :InvoiceLine rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :lineTrack ;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :Track
    ] .

    # Cada fatura tem pelo menos uma linha
    :Invoice rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty :hasLine ;
        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onClass :InvoiceLine
    ] .
}