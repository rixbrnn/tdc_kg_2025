PREFIX : <http://example.org/chinook#>

# Q: What properties and relationships does a Track entity have?
#
# Purpose: ⭐⭐⭐ IMPOSSIBLE IN SQL ⭐⭐⭐ - Dynamic schema exploration
# Complexity: ⭐ (1/5) - Trivial in SPARQL!
# Expected Result: All properties of Track/1 with their values
#
# Demonstrates: SPARQL's schema flexibility - SQL can't do this!
# This is PURE GRAPH MAGIC

SELECT DISTINCT ?property ?value
WHERE {
  :Track/1 ?property ?value .
}
ORDER BY ?property
LIMIT 100

# Example output:
# ?property              | ?value
# ----------------------|---------------------------
# rdf:type              | :Track
# :trackName            | "For Those About To Rock"
# :hasAlbum             | :Album/1
# :hasGenre             | :Genre/1
# :milliseconds         | 343719
# :bytes                | 11170334
# :unitPrice            | 0.99
# :hasMediaType         | :MediaType/1
# :composer             | "Angus Young, Malcolm Young..."

